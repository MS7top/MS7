local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/MS7top/GUI/refs/heads/main/ORIONRED.txt"))()
local Window = OrionLib:MakeWindow({Name = "MS7 || Brookhaven RP", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
	Name = "! الـنهايه !",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Tab:AddSection({
	Name = "! تـم حـذف السـكربت نهـائي !"
})

local Section = Tab:AddSection({
	Name = "! وتـم تنـزيل نسـخه جديـده !"
})

Tab:AddButton({
	Name = "! شـغل السـكربت الـجديد !",
	Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/MS7top/K7/refs/heads/main/K7"))()
  	end    
})

Tab:AddButton({
	Name = "! انسـخ السـكربت الجـديد !",
	Callback = function()
	setclipboard("loadstring(game:HttpGet("https://raw.githubusercontent.com/MS7top/K7/refs/heads/main/K7"))()")
  	end    
})

Tab:AddButton({
	Name = "! انسـخ اسـم السـكربت في الغيـمه !",
	Callback = function()
	setclipboard("K7 HUB")
  	end    
})
