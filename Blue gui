local playerCountLabel = AddTextLabel(Main, "عـدد الاعـبـيـن في السيرفـر هــو " .. #game.Players:GetPlayers())

local function updatePlayerCount()
    playerCountLabel.Text = "عـدد الاعـبـيـن في السيرفـر هــو " .. #game.Players:GetPlayers()
end

game.Players.PlayerAdded:Connect(updatePlayerCount)

game.Players.PlayerRemoving:Connect(updatePlayerCount)

updatePlayerCount()

